{
  "name": "botbrain",
  "version": "0.2.2",
  "description": "Robot brain tools to automate your nodebot",
  "main": "index.js",
  "browser": "static/NeuralNetwork.js",
  "scripts": {
    "test": "mocha",
    "copy": "ncp node_modules/3d-force-graph/dist/3d-force-graph.min.js ./static/lib && ncp node_modules/socket.io-client/socket.io.js ./static/lib",
    "build": "rollup -c && npm run copy",
    "serve": "node -e \"let bb = require('./'); let network = new bb.NeuralNetwork(132, 'tube'); network.output(2); network.channel(0); bb.Toolkit.verbose = true; bb.Toolkit.visualise(network); setInterval(() => network.input(Math.floor(Math.random() * network.size)), 1000);\"",
    "start": "opn http://localhost:8811 && npm run serve"
  },
  "engines": {
    "node": ">=6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sdesalas/botbrain.git"
  },
  "keywords": [
    "robot",
    "brain",
    "neural",
    "network"
  ],
  "author": "Steven de Salas",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/sdesalas/botbrain/issues"
  },
  "homepage": "https://github.com/sdesalas/botbrain#readme",
  "dependencies": {
    "node-static": "^0.7.9",
    "socket.io": "1.4.8"
  },
  "devDependencies": {
    "3d-force-graph": "git+https://github.com/sdesalas/3d-force-graph.git",
    "assert-fuzzy": "^1.0.0",
    "events": "^1.1.1",
    "mocha": "^3.4.2",
    "ncp": "^2.0.0",
    "opn-cli": "^3.1.0",
    "rollup": "^0.41.6",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "uglify-js": "^3.0.15"
  }
}
